<% content_for(:sub_menu) do %>
	<ul>
		<li><%= link_to "Back", admin_galleries_path(@event) %></li>
	</ul>
<% end %>


<div id="content">
	<h1>Edit Event Gallery</h1>

	<div id="tabs" class="ui-tabs">

		<% form_for (@gallery, :url => {:action => "update"},:html => { :multipart => true })  do |f| %>	
	  <%= render :partial => "form", :locals => {:f =>f}%>
	  <p>
	    <%= f.submit 'Edit' %>
	  </p>
		<hr />

		<% end %>
	
	</div>
</div>

<% content_for(:script) do %>

<%= render :partial => "shared/tinymce" %>
<% end %>
